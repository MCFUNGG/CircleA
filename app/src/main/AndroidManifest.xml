            android:name=".setting.MemberCart"
            android:exported="true" />
        <activity
            android:name=".setting.uploadCert"
            android:exported="true" />
        <activity
            android:name=".matching.Matching"
            android:exported="true" />
        <activity
            android:name=".matching.RequestReceivedDetail"
            android:exported="true" />
        <activity
